TIMESTAMPDIFF(MINUTE,(SELECT cadaco.atualizacao_cadaco FROM cadaco where cadaco.cd_cadsol = cadsol.id_cadsol 
and cadaco.cd_cadsta=1 order by 1 desc limit 0,1),cadsol.data_cadsol) as tempo_gasto,
caddir.descricao_caddir AS Direcionamento,


cadsol.desc_cadsol AS Descricao,
cadser.desc_cadser AS Servico,


cadcli.desc_cadcli as Usuario,


cadsta.desc_cadsta AS Status,


caddep.desc_caddep AS Departamento,
 DATE_FORMAT(TIMESTAMPADD(HOUR,TIME_FORMAT(cadser.tempo_cadser,'%H:%i') ,cadsol.data_cadsol),'%d-%m-%Y  %H:%i:%s') as tempo_execucao,
(SELECT cadcli.desc_cadcli FROM cadaco inner join cadcli on cadcli.id_cadcli = cadaco.cd_cadcli order by cadaco.id_cadaco desc limit 0,1 ) as atendente,

cadsol.data_cadsol AS Data_de_Criacao FROM cadsol
inner join cadser on cadser.id_cadser = cadsol.cd_cadser
inner join caddir on caddir.id_caddir = cadser.cd_caddir
inner join cadcli on cadcli.id_cadcli = cadsol.cd_cadcli
inner join caddep on caddep.id_caddep = cadcli.cd_caddep
inner join cadsta on cadsta.id_cadsta = cadsol.cd_cadsta
WHERE 1=1
[ and caddir.id_caddir= ({caddir,VALUE}) ]


</SQLRaw></SQL><QueryColumns><QueryColumn><Name>tempo_gasto</Name><TableName></TableName><ColumnName>TIMESTAMPDIFF(MINUTE,(SELECT cadaco.atualizacao_cadaco FROM cadaco where cadaco.cd_cadsol = cadsol.id_cadsol 
