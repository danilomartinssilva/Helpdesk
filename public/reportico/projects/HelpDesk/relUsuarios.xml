cadcli.ramal_cadcli, caddir.descricao_caddir as Grupo,caddep.desc_caddep as Departamento,
cadperfil.desc_cadper as Perfil
from cadcli
inner join caddep on caddep.id_caddep = cadcli.cd_caddep
inner join caddir on caddir.id_caddir = cadcli.cd_caddir
inner join cadperfil on cadperfil.id_cadper = cadcli.cd_cadper

[ and cadcli.status_cadcli  IN ({situacao_usuario,VALUE}) ]
[ and cadcli.cd_caddir IN ({grupo,VALUE}) ]
[ and cadcli.cd_cadper IN ({perfil,VALUE}) ]
